    "@types/oauth2-server": "^3.0.13",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-oauth-server": "^2.0.0",
    "oauth2-server": "^3.1.1"
  },
  "devDependencies": {
